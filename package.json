		"source-map-support": "^0.5.20",
		"supertest": "^6.1.3",
		"ts-jest": "28.0.8",
		"ts-loader": "^9.4.2",
		"ts-node": "^10.0.0",
		"tsconfig-paths": "4.1.0",

{
	"name": "backend-shop",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"build": "nest build --webpack --webpackPath webpack-hmr.config.js",
		"start": "cross-env NODE_ENV=development npm run build",
		"start:w": "cross-env NODE_ENV=development npm run build --watch",
		"start:d": "cross-env NODE_ENV=development npm run build --debug --watch",
		"start:p": "cross-env NODE_ENV=prod npm run build && node dist/main",
		"format:p": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"format:l": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"format": "concurrently npm:format:*",
		"test": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 npx jest --runInBand --forceExit",
		"test:w": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 npx jest --runInBand --watch",
		"test:c": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 npx jest --coverage --forceExit",
		"test:d": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"jobs": "cross-env NODE_ENV=prod webpack --config webpack-jobs.config.js",
		"jobs:w": "cross-env NODE_ENV=development webpack --config webpack-jobs.config.js --watch"
	},
	"dependencies": {
		"@nestjs/common": "^9.0.0",
		"@nestjs/config": "^2.2.0",
		"@nestjs/core": "^9.0.0",
		"@nestjs/jwt": "^9.0.0",
		"@nestjs/mapped-types": "^1.2.0",
		"@nestjs/mongoose": "^9.2.1",
		"@nestjs/passport": "^9.0.0",
		"@nestjs/platform-express": "^9.0.0",
		"@nestjs/swagger": "^6.1.4",
		"bcryptjs": "^2.4.3",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"concurrently": "^7.6.0",
		"cross-env": "^7.0.3",
		"inquirer": "^9.1.4",
		"lodash": "^4.17.21",
		"mongoose": "^6.8.0",
		"node-schedule": "^2.1.0",
		"passport": "^0.6.0",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.2.0",
		"swagger-ui-express": "^4.6.0"
	},

	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"start": "cross-env NODE_ENV=development nest build --webpack --webpackPath webpack-hmr.config.js",
		"start:w": "cross-env NODE_ENV=development nest build --webpack --webpackPath webpack-hmr.config.js --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"format:p": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"format:l": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"format": "concurrently npm:format:*",
		"test": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules npx jest --runInBand --forceExit",
		"test:w": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules npx jest --runInBand --watch",
		"test:c": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules npx jest --coverage --forceExit",
		"test:d": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
	},

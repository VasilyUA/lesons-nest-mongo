{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"node": "current",
					"esmodules": true
				}
			}
		],
		"@babel/typescript"
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		[
			"@babel/plugin-transform-runtime",
			{
				"corejs": 3
			}
		],
		[
			"transform-inline-environment-variables",
			{
				"include": ["NODE_ENV"]
			}
		],
		[
			"module-resolver",
			{
				"root": ["./back-end"]
			}
		]
	],
	"sourceMaps": "both",
	"sourceType": "unambiguous"
}
